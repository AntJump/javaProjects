package practice;

public class Car{
	private String name;
	private int speed;
	
	public Car(String name, int speed) {
		this.name = name;
		this.speed = speed;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getSpeed() {
		return speed;
	}

	public void setSpeed(int speed) {
		this.speed = speed;
	}

	@Override
	public String toString() {
		return "Car [차 종 = " + name + ", 최고 속도 = " + speed + "]";
	}
	
}